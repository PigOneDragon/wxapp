/* miniprogram/pages/leijishouyi/leijishouyi.wxss */
page {
  font-size: 30rpx;
}
.m-top{
  height: 260rpx;
  background: url(data:image/png;base64,) left bottom no-repeat #009b62;
  background-size: 100% auto;
  text-align: center;
  color: #fff;
}
.txt-1{
  padding-top: 36rpx;
  height: 38rpx;
  line-height: 38rpx;
  display: block;
}
.txt-2{
  height: 88rpx;
  line-height: 88rpx;
  font-size: 72rpx;
}
.m-txtlist{
  padding: 22px 25px 0;
 
}
.m-txtlist .tit{
  width: 100%;
  height: 119rpx;
  line-height: 119rpx;
  background: #fff;
  border-radius: 6rpx;
  padding-left: 30rpx;
  font-size: 30rpx;
  color: #3f3f3f;
  box-sizing: border-box;
}
.m-txtlist .tit image{
  width: 10rpx;
  height: 26rpx;
  margin-right: 18rpx;
}
.item{
  height: 160rpx;
  background: #fff;
  margin-top: 20rpx;
  width: 100%;
  border-radius: 6rpx;
  box-sizing: border-box;
  padding: 38rpx 32rpx 38rpx 40rpx;
  display: flex;
  justify-content: space-between;
}
.item .left .txt-3{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 36rpx;
  color: #333333;
  display: block;
}
.item .left .data{
  margin-top: 20rpx;
  height: 24rpx;
  line-height: 24rpx;
  color: #aaaaaa;
  font-size: 24rpx;
}
.item .right{
  color: #009b62;
  font-size: 36rpx;
}